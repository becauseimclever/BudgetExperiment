@using BudgetExperiment.Client.Services
@inject ThemeService ThemeService
@implements IDisposable

<svg class="icon @Class"
     width="@Size"
     height="@Size"
     viewBox="0 0 24 24"
     fill="none"
     stroke="currentColor"
     stroke-width="@StrokeWidth"
     stroke-linecap="round"
     stroke-linejoin="round"
     aria-hidden="@(string.IsNullOrEmpty(Title) ? "true" : null)"
     role="@(string.IsNullOrEmpty(Title) ? null : "img")"
     aria-label="@Title">
    @if (!string.IsNullOrEmpty(Title))
    {
        <title>@Title</title>
    }
    @((MarkupString)GetIconPath())
</svg>

@code {
    /// <summary>
    /// The name of the icon to display.
    /// </summary>
    [Parameter]
    public string Name { get; set; } = string.Empty;

    /// <summary>
    /// The size of the icon in pixels.
    /// </summary>
    [Parameter]
    public int Size { get; set; } = 20;

    /// <summary>
    /// Additional CSS classes to apply to the icon.
    /// </summary>
    [Parameter]
    public string? Class { get; set; }

    /// <summary>
    /// Stroke width for the icon paths.
    /// </summary>
    [Parameter]
    public double StrokeWidth { get; set; } = 2;

    /// <summary>
    /// Optional title/tooltip for the icon (also used for accessibility).
    /// </summary>
    [Parameter]
    public string? Title { get; set; }

    /// <inheritdoc />
    protected override void OnInitialized()
    {
        ThemeService.ThemeChanged += OnThemeChanged;
    }

    /// <inheritdoc />
    public void Dispose()
    {
        ThemeService.ThemeChanged -= OnThemeChanged;
    }

    private void OnThemeChanged(string theme)
    {
        InvokeAsync(StateHasChanged);
    }

    private string GetResolvedIconName()
    {
        return ThemeService.GetThemedIcon(Name);
    }

    private string GetIconPath()
    {
        var resolvedName = GetResolvedIconName();
        return GetIconPathByName(resolvedName);
    }

