{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "AutoMigrate": true,
    "MigrationTimeoutSeconds": 300
  },
  "CsvImportDeduplication": {
    "FuzzyDateWindowDays": 3,
    "MaxLevenshteinDistance": 5,
    "MinJaccardSimilarity": 0.6
  }
}
